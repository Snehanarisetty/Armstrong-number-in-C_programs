#include <stdio.h>
int main() {
    int n , rem, sum = 0,a;
    printf("Enter a three-digit integer: ");
    scanf("%d", &n);
    a = n;
    while (n != 0) {
        rem = n % 10;
       sum =sum + rem * rem * rem;
       n = n/10;
    }
    if (a == sum)
        printf("%d is an Armstrong number.", n);
    else
        printf("%d is not an Armstrong number.", n);
    return 0;
}
